{
  "security": {
    "contentSecurityPolicy": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline';",
    "xssProtection": "1; mode=block",
    "hsts": {
      "maxAge": 31536000,
      "includeSubDomains": true
    }
  },
  "performance": {
    "caching": {
      "enabled": true,
      "maxAge": 3600
    }
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}